{
  "name": "gatsby-starter-z1",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Z1 Lab <lab@z1.digital>",
  "dependencies": {
    "gatsby": "^2.3.32",
    "gatsby-image": "^2.0.40",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-source-filesystem": "^2.0.32",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.1.18",
    "gatsby-transformer-yaml-full": "^0.1.0",
    "minireset.css": "^0.0.4",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "sass-mq": "^5.0.0",
    "sass-rem": "^2.0.1"
  },
  "devDependencies": {
    "@nice-labs/typed-css-modules": "^1.2.1",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "docz": "^1.1.0",
    "docz-theme-default": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gatsby-plugin-svgr": "^2.0.2",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "typescript": "^3.4.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run tschecker && gatsby build",
    "develop": "gatsby develop",
    "format": "eslint --fix ./src/**/*.{ts,tsx,js} && prettier --write ./src/**/*.{scss,css,json}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "tschecker": "tsc",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx,js}": [
      "eslint --fix",
      "git add"
    ],
    "./src/**/*.{css,scss,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/z1digitalstudio/gatsby-starter-z1"
  },
  "bugs": {
    "url": "https://github.com/z1digitalstudio/gatsby-starter-z1/issues"
  }
}
